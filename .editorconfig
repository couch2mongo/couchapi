
; https://editorconfig.org/
;
; Here we define the standards for Editors, please use plugins that
; support Editor Config. It will then make sure you format your files
; correctly.
root = true

[*]
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space
end_of_line = lf

[*.rs]
indent_size = 4

[*.ts]
indent_size = 2
max_line_length = 120
